{
    "metadata": [
        {
            "src": [
                {
                    "files": ["src/**.csproj"]
                }
            ],
            "dest": "api",
            "disableGitFeatures": false,
            "disableDefaultFilter": false
        }
    ],
    "build": {
        "content": [
            {
                "files": [
                    "01.education/**.md",
                    "01.education/**.yml",
                    "02.projects/**.md",
                    "02.projects/**.yml",
                    "*.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": ["images/**", "resources/**"]
            }
        ],
        "dest": "docs",
        "globalMetadataFiles": [],
        "globalMetadata": {
            "_appName": "T3kla's Portfolio",
            "_appTitle": "T3kla",
            "_appLogoPath": "images/tkl_logo.png",
            "_appFaviconPath": "images/tkl_logo.png",
            "_appFooter": "Need4Oxygen<br/>Generated by <b>DocFX</b>",
            "_enableSearch": true,
            "_disableAffix": true,
            "_disableSideFilter": true,
            "_disableBreadcrumb": true
        },
        "fileMetadataFiles": [],
        "template": ["default", "templates/discordfx"],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}
